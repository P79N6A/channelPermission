	<select id="queryRepairsInvoiceId" parameterType="java.lang.String" resultMap="BaseResultMap">
	SELECT
		r.id,
		i.id invoiceId,
		p.id orderProductId
	FROM
		OrderRepairs r,
		OrderProducts p,
		Invoices i
	WHERE
		r.orderProductId = p.id
	AND i.orderProductId = p.id
	and r.repairSn=#{repairSn}
