<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.haier.stock.dao.stock.ReturnReasonDao">
    <select id="getIdByReturnReason" parameterType="String" resultType="String">
        SELECT
            `id`
        FROM `reason_for_return`
        WHERE `return_reason` = #{returnReason}
    </select>
</mapper>