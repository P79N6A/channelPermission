<script>

    function onQuery(pageIndex){
        //增加网单查询
        var cOrderSn = $( '#cOrderSn' ).val();
//        if (cOrderSn == ""){
//            alert("网单号不能为空")
//            return false;
//        }
        jQuery.ajax({
            url: "/orderclosehistory/getKpiOrderOntimeRateReverseReportDetailList.html",
            data: "repairId=$!request.getParameter('repairId')&cOrderSn="+cOrderSn+"&pageIndex="+pageIndex+"&pageSize=10&sign=other&nodeType=orderclose",
            type: "post",
            success: function(html) {
                $("#html").html(html);
            }
        });
        return false;
    }

    function onExport(){
        $('#filterForm').prop("action","/orderclosehistory/exportKpiOrderOntimeRateReverseReportDetail.html");
        var form = document.getElementById("filterForm");
        form.submit();
    }
</script>

<form id="filterForm" name="filterForm">
    <input id="nodeType" name="nodeType" type="hidden" value="nodeType"/>
</form>


<div class="search-panel" style="font-size: 11px;">
    <!--增加网单查询开始-->
    <div class="search-row_nowidth mar_r6px">
        <span class="search-row-label_80">网单号:</span>
        <input type="text" class="easyui-textbox" id="cOrderSn" name="cOrderSn" value=""/>
        <a href="#" class="easyui-linkbutton" onclick="onQuery(1)"
           data-options="iconCls:'icon-search',plain:true">查询</a>
        <a href="#" class="easyui-linkbutton" onclick="onExport()"
           data-options="iconCls:'icon-save',plain:true">导出</a>
    </div>

</div>
