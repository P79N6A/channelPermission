            if('$!userName' !=''){
                $('#areaTrade').empty();
                $("#areaCommissioner").append("<option value='$!userName'>$!userName</option>");
                if('$!userTrade'==''){
                    $("#areaCommissioner").change();
                }else{
                    var tradeArray='$!userTrade'.split(',');
                    if(tradeArray.length>1){
                        $("#areaTrade").append("<option value=''>请选择</option>");
                        $("#areaTrade").append("<option value='-1'>全部</option>");
                        for (var t=0;t<tradeArray.length;t++){
                            $("#areaTrade").append("<option value="+tradeArray[t]+">"+tradeArray[t]+"</option>");
                        }
