                        $("#areaCommissioner").append("<option value='-1'>全部</option>");
                        jQuery.each(result.data,function(i, v){
                            $("#areaCommissioner").append("<option value=" + v.value + ">" + v.value + "</option>");
                        });
                    });
                }else{
                    $('#areaCommissioner').empty();
                    jQuery.getJSON("/orderClose/getTradeAllCommissioner", function(result){
                        $("#areaCommissioner").append("<option value=''>请选择</option>");
                        $("#areaCommissioner").append("<option value='-1'>全部</option>");
                        jQuery.each(result.data,function(i, v){
                            $("#areaCommissioner").append("<option value=" + v.value + ">" + v.value + "</option>");
                        });
                    });
                }
            }
        });

        function SearchClear() {
            $("#sourceCode").val("");
            $("#sourceName").val("");
        }
        function te() {
            var codevalue = "";
            var namevalue = "";
            $.each($('input:checkbox:checked'), function () {
                if ($(this).attr("name") == "source_Index") {
                    namevalue = namevalue + $(this).val() + ";";
                    codevalue = codevalue + $(this).attr("id") + ";";
                }
            });
            $("#sourceCode").val(codevalue);
            $("#sourceName").val(namevalue);
            $("#dialog-message").dialog("close");
        }


        function onQuery() {
            var url;

            if($( '#orderSn' ).val()==''){
                if(!checkIsOneMonth()){
                    alert("时间跨度不能超过一个月");
                    $('#queryButton').removeAttr("disabled");
                    return false;
                }
            }

            if($("#queryHistory").is(":checked")){
                //历史信息查询
                url = "/orderclosehistory/getKpiOrderOntimeRateReverseReportList.html";
            }else{
                //时时信息查询
                url = "/orderClose/getOrderOntimeRateReverseReportList";
            }

            $('#orderSn').val($('#orderSn').val().trim());
            jQuery.ajax({
                url: url,
                data: $('#filterForm').serialize(),
                type: "GET",
                success: function (data) {
                    $("#list").html(data);
                    $('#queryButton').removeAttr("disabled");
                }
            });
            return false;
        }
        function getSource() {
            jQuery("#dialog-message").dialog("open");
        }
        $("#dialog-message").dialog({
            autoOpen: false,
            modal: true,
            closeOnEscape: false,
            resizable: false,
            minHeight: 200,
            height: 410,
            width: 810,
            title: '订单来源列表'
        });

        function onExport() {
            if($("#queryHistory").is(":checked")){
                //历史导出
                jQuery('#filterForm').prop('action', '/orderclosehistory/exportKpiOrderOntimeRateReverseReport.html');
                var form = document.getElementById("filterForm");
                form.submit();
            }else{
                jQuery('#filterForm').prop('action', '/orderClose/exportOrderOntimeRateReverseReport.html');
                var form = document.getElementById("filterForm");
                form.submit();
            }
        }

        function hiddenSelect(obj) {
            jQuery("select").each(function () {
                jQuery(this).attr("disabled", "disabled");
            });
            if (obj.value == 'PCR') {
                $("#province").removeAttr("disabled");
                $("#city").removeAttr("disabled");
                $("#region").removeAttr("disabled");
            } else if (obj.value == 'A') {
//                $("#area").removeAttr("disabled");
                $("#areaCommissioner").removeAttr("disabled");
                $("#areaTrade").removeAttr("disabled");
                $("#areaSCode").removeAttr("disabled");
            } else if (obj.value == 'FDA') {
                $("#fdArea").removeAttr("disabled");
                $("#fdAreaTrade").removeAttr("disabled");
                $("#fdAreaSCode").removeAttr("disabled");
            }
        }

        function checkIsOneMonth(){//检查是否超过一个月
            var startDate = $("#startDate").datebox("getValue");
            var endDate = $("#endDate").datebox("getValue");
            var startYear = startDate.substring(0,4), startMonth = startDate.substring(5,7), startDay = startDate.substring(8,10);
            var endYear = endDate.substring(0,4), endMonth = endDate.substring(5,7), endDay = endDate.substring(8,10);
            if(endYear - startYear > 1)
                return false;
            else if(endYear - startYear == 1){
                if(endMonth != '01' || startMonth != '12')
                    return false;
                else if(endDay > startDay)
                    return false;
                else
                    return true;
            }else if(endYear - startYear == 0){
                if(endMonth - startMonth > 1)
                    return false;
                else if(endMonth - startMonth == 1 && endDay > startDay)
                    return false;
                else
                    return true;
            }
        }
    </script>

</head>


