<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head id="Head1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <title>PO查询</title>
	<link rel="stylesheet" href="../static/css/jqueryui/base/jquery.ui.all.css"/>
    <link rel="stylesheet" type="text/css" href="../static/third/easyui/themes/gray/easyui.css">
    <link rel="stylesheet" type="text/css" href="../static/third/easyui/themes/icon.css">
    <script type="text/javascript" src="../static/third/jquery-1.8.3.min.js"></script>
    <script type="text/javascript" src="../static/third/easyui/jquery.easyui.min.js"></script>
    <script type="text/javascript" src="../static/third/easyui/locale/easyui-lang-zh_CN.js"></script>


</head>
<body>
<div class="easyui-layout" data-options="fit : true,border : false">
	<div data-options="region:'north',title:'查询条件',border:false" style="display:none;height: 182px;overflow: auto;" align="left">
		<form id="filterForm" action="#"> 
   		  	<table >
	    	<tr>
	    		<td>  到货开始周</td>
	        	<td style="position: relative"><input id="report_year_week_start_txt" name ="report_year_week_start_txt"  style="position:absolute;width:130px;height: 16px;" readonly="readonly" class="text" zindex="1"/>
	        		<input id="report_year_week_start" name="report_year_week_start"
	               	data-options="formatter:myformatter_report_start,parser:myparser" editable="false"/>
				</td>
				<td>结束周</td>
	     		<td style="position: relative">
	  		  		 <input id="report_year_week_end_txt" name ="report_year_week_end_txt"  style="position:absolute;width:130px;height: 16px;" readonly="readonly" class="text" zindex="1"/>
	     	 		 <input id="report_year_week_end" name="report_year_week_end"
	              		 data-options="formatter:myformatter_report_end,parser:myparser" editable="false"/>
				</td>
				<td> 渠道</td>
	     		<td> <input id="channel" name="channel" class="easyui-combobox" style="width:160px"/>
				</td>
				<td>  品类</td>
		       	<td> 
		       		<input id="cbsCategory" name="cbsCategory" class="easyui-combobox" style="width:160px"/> 
				</td>
				<td>产品组</td>
	        	<td> <input id="product_group" name="product_group" class="easyui-combobox" style="width:160px"/>
				</td>
	    	</tr>
			<tr>
			<td>  WP订单号</td>
       		<td> <input id="order_id" name="order_id" class="easyui-textbox" style="width:160px" /> 
			</td>
			<td>  品牌</td>
	        <td> <input id="brand" name="brand" class="easyui-combobox" style="width:160px"/>
			</td>
			<td> WA库码</td>
	        <td> <input id="storage_id" name="storage_id" class="easyui-textbox" style="width:160px"/>
			</td>
			<td>物料编码</td>
	        <td> <input id="materials_id" name="materials_id" class="easyui-textbox" style="width:160px"/>   
			</td>
			<td> 型号</td>
	        <td> <input id="materials_desc" name="materials_desc" class="easyui-textbox" style="width:160px"/>
			</td>
			</tr>
			<tr>
			<td>销售单号</td>
	        <td> <input id="bill_order_id" name="bill_order_id" class="easyui-textbox" style="width:160px"/>
			</td>
			<td> 提单开始日期</td>
	        <td> <input id="bill_time_start" name="bill_time_start"
	               data-options="formatter:myformatter,parser:myparser" editable="false"/>
			</td>
			<td>结束日期</td>
	        <td> <input id="bill_time_end" name="bill_time_end"
	               data-options="formatter:myformatter,parser:myparser" editable="false"/></td>
			
			<td>入库开始日期</td>
	        <td> <input id="datestorge_start" name="datestorge_start"
	               data-options="formatter:myformatter,parser:myparser" editable="false"/>
			</td>
			<td>结束日期</td>
	        <td> <input id="datestorge_end" name="datestorge_end"
	               data-options="formatter:myformatter,parser:myparser" editable="false"/>
			</td>
			</tr>
			<tr>
			<td> SO</td>
	        <td> <input id="so_id" name="so_id" class="easyui-textbox" style="width:160px" />  
			</td>
			<td> DN</td>
	        <td> <input id="dn_id" name="dn_id" class="easyui-textbox" style="width:160px" /> 
			</td>
			<td>  状态</td>
	   		<td>  <input id="flow_flag" name="flow_flag" class="easyui-combobox" style="width:160px"/>  
			</td>
			</tr>
	   		<tr>
	    		<td  colspan="10">
				<a id='search' href="#" class="easyui-linkbutton" iconCls="icon-search">查询</a>
	      	    <a id='export' href="#" class="easyui-linkbutton" iconCls="icon-save">导出</a>
				<a id='exportall' href="#" class="easyui-linkbutton" iconCls="icon-save">全部导出</a>
				</td>
	       </tr>
	       </table>
	 		<input id="report_start_week_hidden" name="report_start_week_hidden" type="hidden">
	 		<input id="report_end_week_hidden" name="report_end_week_hidden" type="hidden">
	 		<input id="ed_channel_id_hidden" name="ed_channel_id_hidden" type="hidden">
	        <input id="product_group_id_hidden" name="product_group_id_hidden" type="hidden">
			<input id="wp_order_id_hidden" name="wp_order_id_hidden" type="hidden">
	        <input id="storage_id_hidden" name="storage_id_hidden" type="hidden">
	        <input id="flow_flag_hidden" name="flow_flag_hidden" type="hidden">
	        <input id="materials_id_hidden" name="materials_id_hidden" type="hidden">
	        <input id="bill_time_start_hidden" name="bill_time_start_hidden" type="hidden">
	        <input id="bill_time_end_hidden" name="bill_time_end_hidden" type="hidden">
	        <input id="bill_order_id_hidden" name="bill_order_id_hidden" type="hidden">
	        <input id="so_id_hidden" name="so_id_hidden" type="hidden">
			<input id="dn_id_hidden" name="dn_id_hidden" type="hidden">
	        <input id="datestorge_start_hidden" name="datestorge_start_hidden" type="hidden">
			<input id="datestorge_end_hidden" name="datestorge_end_hidden" type="hidden">
			<input id="brand_id_hidden" name="brand_id_hidden" type="hidden">
			<input id="category_id_hidden" name="category_id_hidden" type="hidden">
			<input id="materials_desc_hidden" name="materials_desc_hidden" type="hidden">
			<!-- 下面隐藏控件用来存放点击导出后的数据 -->
			<input id="exportData" name="exportData" type="hidden"/>
    </form>
	</div>
	<div data-options="region:'center',title:'PO查询结果',border:false">
		<table id="dataGrid" ></table>
	</div>
</div>

<script type="text/javascript"></script>
<script src="../static/js/report/t2OrderQueryPoList.js"></script>
</body>
</html>