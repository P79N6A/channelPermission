<script type="text/javascript">
    function slideUpSlideClk(t) {
        var $this =
        $(t);
        if ($this.hasClass('open')) {
                $this.removeClass('open');
            setTimeout(function () {
                    $this.addClass('close');
            }, 0);
            $(".search-panel").slideUp();
            $('#gridView').datagrid();
        } else {
                $this.removeClass('close');
            setTimeout(function () {
                    $this.addClass('open');
            }, 100);
            $(".search-panel").slideDown();
        }
        setTimeout(function () {
            $('#gridView').datagrid('resize');
        }, 500);
    }

</script>
