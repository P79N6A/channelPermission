    function proof(v){
        var special = new RegExp("[`~!@#$^&*()=|{}':;',\\[\\].<>/?~！@#￥……&*（）——|{}【】‘；：”“'。，、？]");
        var chinese = new RegExp("[\u4e00-\u9fa5]");
        if(special.test(v) || chinese.test(v)){
            return true;
        }
    }
    function proofnum(num){
        var special = new RegExp("^[0-9]+(.[0-9]{1,2})?$");
        if(!special.test(num) ){
            return true;
        }
    }

