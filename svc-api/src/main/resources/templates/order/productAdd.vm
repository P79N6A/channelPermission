            success: function(data) {
                var json = JSON.parse(data);
                var el = $("select.city1");
                $("#cityId").empty();
                for (key in json.data) {
                    $("#cityId").append("<option value=\""+json.data[key]["cityId"]+"\">"+json.data[key]["cityName"]+"</option>");
                }
            }
        });
