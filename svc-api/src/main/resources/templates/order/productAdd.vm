    function addCity() {
        var src = document.getElementById('cityId');
        var dest = document.getElementById('cityIds');
        add(src,dest);
    }

    function addSimProductIds() {
        var src = document.getElementById('ProductIds');
        var dest = document.getElementById('simProductIds');
